#include <stdio.h>
#include <stdlib.h>

/*
Elabore um programa em C para ler do teclado um valor inteiro entre 1 e 10 e apresentar a
tabuada.
*/

int main()
{
    int valor;
    int resultado;
    
    do{
        printf("\tDIGITE UM VALOR ENTRE 1 E 10: ");
        scanf("%d", &valor);
    }
    while(valor <= 0 || valor > 10);
    
    for(int i = 1; i <= 10; i++){
        resultado = valor*i;
        printf("SaÃ­da: %d\n", resultado);
    }
    
    return 0;
}
